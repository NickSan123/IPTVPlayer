<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp">
    <EditText
        android:id="@+id/nome_canal_edittext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Nome do Canal" />

    <EditText
        android:id="@+id/url_stream_edittext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="URL do Stream" />

    <EditText
         android:id="@+id/url_imagem_edittext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="URl da logo"/>
  <Button
    android:id="@+id/bt_salvar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:onClick=""
    android:text="Salvar"/>

</LinearLayout>